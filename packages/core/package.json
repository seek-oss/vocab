{
  "name": "@vocab/core",
  "version": "1.6.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vocab.git",
    "directory": "packages/core"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "@vocab-private/monorepo": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./icu-handler": {
      "@vocab-private/monorepo": "./src/icu-handler.ts",
      "import": "./dist/icu-handler.mjs",
      "require": "./dist/icu-handler.cjs"
    },
    "./runtime": {
      "@vocab-private/monorepo": "./src/runtime.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    "./translation-file": {
      "@vocab-private/monorepo": "./src/translation-file.ts",
      "import": "./dist/translation-file.mjs",
      "require": "./dist/translation-file.cjs"
    },
    "./package.json": "./package.json"
  },
  "author": "SEEK",
  "license": "MIT",
  "files": [
    "dist",
    "icu-handler",
    "runtime",
    "translation-file"
  ],
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.0.10",
    "chokidar": "^4.0.0",
    "debug": "^4.3.1",
    "fast-glob": "^3.2.4",
    "fastest-validator": "^1.9.0",
    "find-up": "^5.0.0",
    "intl-messageformat": "^10.0.0",
    "picocolors": "^1.0.0",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "./icu-handler": {
        "import": "./dist/icu-handler.mjs",
        "require": "./dist/icu-handler.cjs"
      },
      "./runtime": {
        "import": "./dist/runtime.mjs",
        "require": "./dist/runtime.cjs"
      },
      "./translation-file": {
        "import": "./dist/translation-file.mjs",
        "require": "./dist/translation-file.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
