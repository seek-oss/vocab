{
  "name": "@vocab/cli",
  "version": "2.1.12",
  "description": "A CLI for generating typed Vocab translations and interacting with 3rd-party localization services",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vocab.git",
    "directory": "packages/cli"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "@vocab-private/monorepo": "./src/index.ts",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "vocab": "bin.js"
  },
  "files": [
    "dist",
    "bin.js"
  ],
  "author": "SEEK",
  "license": "MIT",
  "dependencies": {
    "@vocab/core": "workspace:^",
    "@vocab/phrase": "workspace:^",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.cjs",
      "./package.json": "./package.json"
    }
  }
}
