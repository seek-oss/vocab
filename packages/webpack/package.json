{
  "name": "@vocab/webpack",
  "version": "1.2.13",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vocab.git",
    "directory": "packages/webpack"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./chunk-name": {
      "import": "./dist/chunk-name.mjs",
      "require": "./dist/chunk-name.js"
    },
    "./loader": {
      "import": "./dist/loader.mjs",
      "require": "./dist/loader.js"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "require": "./dist/web.js"
    },
    "./package.json": "./package.json"
  },
  "author": "SEEK",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vocab/core": "workspace:^",
    "cjs-module-lexer": "^2.0.0",
    "debug": "^4.3.1",
    "es-module-lexer": "^1.0.0",
    "picocolors": "^1.0.0",
    "virtual-resource-loader": "workspace:^"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "webpack": "^5.37.0"
  },
  "peerDependencies": {
    "webpack": "^5.37.0"
  }
}
