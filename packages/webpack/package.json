{
  "name": "@vocab/webpack",
  "version": "1.2.16",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vocab.git",
    "directory": "packages/webpack"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "@vocab-private/monorepo": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./chunk-name": {
      "@vocab-private/monorepo": "./src/chunk-name.ts",
      "require": "./dist/chunk-name.cjs",
      "import": "./dist/chunk-name.mjs"
    },
    "./loader": {
      "@vocab-private/monorepo": "./src/loader.ts",
      "require": "./dist/loader.cjs",
      "import": "./dist/loader.mjs"
    },
    "./web": {
      "@vocab-private/monorepo": "./src/web.ts",
      "require": "./dist/web.cjs",
      "import": "./dist/web.mjs"
    },
    "./package.json": "./package.json"
  },
  "author": "SEEK",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vocab/core": "workspace:^",
    "cjs-module-lexer": "^2.0.0",
    "debug": "^4.3.1",
    "es-module-lexer": "^1.0.0",
    "picocolors": "^1.0.0",
    "virtual-resource-loader": "workspace:^"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "webpack": "^5.37.0"
  },
  "peerDependencies": {
    "webpack": "^5.37.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./chunk-name": {
        "require": "./dist/chunk-name.cjs",
        "import": "./dist/chunk-name.mjs"
      },
      "./loader": {
        "require": "./dist/loader.cjs",
        "import": "./dist/loader.mjs"
      },
      "./web": {
        "require": "./dist/web.cjs",
        "import": "./dist/web.mjs"
      },
      "./package.json": "./package.json"
    }
  }
}
